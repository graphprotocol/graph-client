{
  "name": "@graphprotocol/client-apollo",
  "version": "0.0.2",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphprotocol/graph-client.git",
    "directory": "packages/apollo"
  },
  "scripts": {
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "thegraph",
    "graphql",
    "client"
  ],
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",

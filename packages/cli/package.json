{
  "name": "@graphprotocol/client-cli",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphprotocol/graph-client.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "thegraph",
    "graphql",
    "client"
  ],
  "license": "MIT",
  "bin": {
    "graphclient": "./dist/bin.js"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "buildOptions": {
    "bin": {
      "graphclient": {
        "input": "src/bin.ts"
      }
    }
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "@graphql-mesh/cli": "0.71.1",
    "@graphql-mesh/graphql": "0.26.1",
    "tslib": "2.4.0",
    "@graphprotocol/client-auto-pagination": "1.0.0",
    "@graphprotocol/client-block-tracking": "1.0.0",
    "@graphprotocol/client-add-source-name": "1.0.0"
  },
  "devDependencies": {
    "graphql": "16.5.0"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}

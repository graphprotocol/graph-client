{
  "name": "@graphprotocol/client",
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/graphprotocol/graph-client.git",
    "directory": "packages/client"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "prepack": "bob prepack",
    "check": "tsc --pretty --noEmit"
  },
  "keywords": [
    "thegraph",
    "graphql",
    "client"
  ],
  "license": "MIT",
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "dependencies": {
    "tslib": "2.3.1",
    "@graphql-tools/wrap": "8.4.0",
    "@graphql-tools/utils": "8.6.1",
    "cross-undici-fetch": "0.1.24"
  },
  "devDependencies": {
    "@types/node": "17.0.19",
    "graphql": "16.3.0",
    "bob-the-bundler": "1.6.1"
  },
  "peerDependencies": {
    "graphql": "^15.2.0 || ^16.0.0"
  }
}
